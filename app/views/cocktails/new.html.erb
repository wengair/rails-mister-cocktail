<h1>Cocktails#new</h1>
<p>Find me in app/views/cocktails/new.html.erb</p>
<div class="container new-coc">
  <%= simple_form_for @cocktail do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit "add a cocktail", class: "btn btn-primary" %>
  <% end %>
</div>
